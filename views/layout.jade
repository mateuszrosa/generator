doctype html
mixin item(title, url)
  li
    a(href=url class=path == url ? 'active' : '')=title

html
  head
    title= title
    link(rel='stylesheet', href='/stylesheets/style.css')
  body
    ul
      +item('Strona Główna', "/")
      +item('Newsy', "/news")
      +item('Quiz', "/quiz")
      +item('Admin', "/admin")
     


    block content